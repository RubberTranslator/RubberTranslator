<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="rootPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rubbertranslator.mvp.view.controller.impl.FilterController">
   <children>
      <ListView fx:id="processList" editable="true" VBox.vgrow="ALWAYS" />
      <HBox alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT" spacing="10.0">
         <children>
            <CheckBox fx:id="openCheckBox" mnemonicParsing="false" text="开启" />
            <Button fx:id="addBt" mnemonicParsing="false" onAction="#onAddButtonClick" text="添加" />
            <Button fx:id="removeBt" mnemonicParsing="false" onAction="#onRemoveButtonClick" text="删除" />
         </children>
      </HBox>
   </children>
</VBox>
